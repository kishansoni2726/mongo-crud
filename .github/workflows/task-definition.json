{
    "family": "crud",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",  
    "memory": "512",
    "containerDefinitions": [
      {
        "name": "crud",
        "image": "254774371055.dkr.ecr.us-east-1.amazonaws.com/crud:latest",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
        {
            "containerPort": 3000,
            "protocol": "tcp"
        }
      ] 
    }
    ],
     "executionRoleArn": "arn:aws:iam::254774371055:role/ecsTaskExecutionRole",
  "taskRoleArn": null
}
  
